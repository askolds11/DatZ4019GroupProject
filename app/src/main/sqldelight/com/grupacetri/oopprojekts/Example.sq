-- create table
CREATE TABLE example (
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    custom_string TEXT
);

-- seed table
INSERT INTO example (custom_string) VALUES
('Item 1'),
('Wowzers!'),
('Boom'),
('lolololo'),
('oooooooooooooo'),
('aaaaaaaaaaaaaa'),
('ghqiop');

-- functions and their respective queries
selectList:
SELECT *
FROM example;

insert:
INSERT INTO example(custom_string)
VALUES (?);

delete:
DELETE FROM example WHERE id = ?;